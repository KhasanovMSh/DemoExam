        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Профессиональный технический университет" MinHeight="600" MinWidth="1000" Icon="Resources/Icons/logo.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FF004165">
            
        </Grid>
        <Grid Grid.Row="1" Background="#FF009FDA">
            <Frame x:Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        </Grid>
    </Grid>
</Window>
